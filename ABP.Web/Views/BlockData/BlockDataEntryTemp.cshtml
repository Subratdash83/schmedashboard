    /*-------Start Input Text Validations of the Form--------*/

    function validateKeyPress(event) {
        const key = event.key;
        const input = event.target;

        // Prevent typing "e" or "E"
        if (key === 'e' || key === 'E') {
            event.preventDefault();
        }
    }

    function validateInput(input) {
        // Ensure value is positive
        if (input.value < 0) {
            input.value = 0;
        }
        const sanitizedValue = input.value.replace(/\D/g, '');

        // Limit to 16 digits
        if (sanitizedValue.length > 16) {
            input.value = sanitizedValue.substr(0, 16);
        } else {
            input.value = sanitizedValue;
        }
    }
    function validateKeyPress1(event) {
        // Allow backspace, delete, left arrow, right arrow, and decimal point on key-press
        if (event.keyCode === 8 || event.keyCode === 46 || event.keyCode === 37 || event.keyCode === 39 || event.key === ".") {
            return;
        }
        const inputElement = event.target;
        const sanitizedValue = inputElement.value.replace(/[^0-9]/g, ''); // Remove non-numeric characters
        const newValue = sanitizedValue + event.key;

        // Prevent input if the length exceeds 16 digits
        if (newValue.replace(".", "").length > 16) {
            event.preventDefault();
        }
    }

    function validateInput1(inputElement) {
        // Remove non-numeric characters except for the decimal point
        inputElement.value = inputElement.value.replace(/[^0-9.]/g, "");

        // Ensure that there is only one decimal point and it's not at the beginning
        const parts = inputElement.value.split(".");
        if (parts.length > 2 || inputElement.value.startsWith(".")) {
            inputElement.value = parts[0] + "." + parts.slice(1).join("");
        }

        // If there's a decimal part, round it to two decimal places
        if (parts.length === 2) {
            const decimalPart = parts[1].substr(0, 2); // Limit to two decimal places
            inputElement.value = parts[0] + (decimalPart ? "." + decimalPart : "."+"");
        }

        // Limit the total length to 16 digits
        if (inputElement.value.replace(".", "").length > 16) {
            inputElement.value = inputElement.value.substr(0, 16);
        }
    }

