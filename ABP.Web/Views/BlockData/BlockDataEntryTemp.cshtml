    function LoadData() {
        $.ajax({
            url: "/BlockData/GetBdoDtaAlert",
            type: "GET",
            contentType: "application/json; charset=utf-8",
            datatype: "json",
            success: function (data) {

                var table = "<table class='tblResult5 table' id=tblResult5><thead > <tr><th width='80px'>Year</th><th width='100px'>Frequency</th><th>Total Datapoint</th><th>Entered Datapoint</th><th>Not Entered</th><th>Remarks</th><th>Action</th></tr></thead><tbody>";
                for (var i = 0; i < data.length; i++) {

                    var row = "<tr>";

                    row += '<td>' + data[i].year + '</td>';
                    row += '<td>' + data[i].frequency + '</td>';
                    row += '<td>' + data[i].dpcount + '</td>';
                    row += '<td>' + data[i].nonzerodpcount + '</td>';
                    row += '<td>' + data[i].hndpcount + '</td>';
                    row += '<td>' + data[i].remarks + '</td>';
                    row += '<td>' + "<input type='button' value='Proceed for data entry' class='btn btn-success btn-sm' onclick='RediecttoPage(" + data[i].year + ")'/>" + '</td>';
                    row += '</tr>';
                    table += row;
                }
                table += '</tbody></table>';
                $('#tblresult').html(table);
                $("#PreviewDetails").modal('show');
            },
            error: function () {
                alert("Content load failed.");
            }

        });
    }
    function submitData(status) {

        if ((status == '2' || status == '0')) {
            var currentDate = new Date();
            var months = ["January", "February", "March", "April", "May", "June",
                "July", "August", "September", "October", "November", "December"
            ];
            var prev = new Date(currentDate.getFullYear(), currentDate.getMonth() - 1, 1);
            var viewingMonth = months[prev.getMonth()];
            //Getting all ids of all the tbodys to get all the row data for the insertion
            var Ids = []; //array declaring to store the ids of all the tbodys
            $('tbody').each(function () {
                var itm = {};
                itm.id = $(this).attr('id');
                Ids.push(itm)
            });
            var isallowed = false;
            var Items = []; //array declaring to store the records to send it to controller
            for (var j = 0; j < Ids.length; j++) {
                var tbl = document.getElementById(Ids[j].id);
                for (var i = 0; i < tbl.rows.length; i++) {
                    var item1 = {};
                    item1.FREQUENCYID = parseInt(tbl.rows[i].children[11].textContent);;
                    item1.CONTROLID = parseInt(tbl.rows[i].children[1].textContent);
                    item1.CONTROLVALUE = parseFloat(tbl.rows[i].children[4].children[0].value);
                    if (Number.isNaN(item1.CONTROLVALUE)) {
                        item1.CONTROLVALUE = null;
                    }
                    else {
                        item1.CONTROLVALUE = item1.CONTROLVALUE;
                        isallowed = true;
                    }
                    item1.CONTROLNAME = tbl.rows[i].children[5].textContent;
                    item1.PANELID = parseFloat(tbl.rows[i].children[6].textContent);
                    item1.FROMDATE = $("#lblFromDate").text();
                    item1.TODATE = $("#lblToDate").text();
                    if (lblFrequencyId == '5') {
                        item1.FREQUENCYNO = $("#ddlYear").val();
                        item1.FREQUENCYVALUE = $("#ddlYear option:selected").text();
                    }
                    else {
                        item1.FREQUENCYNO = $("#ddlMonth").val();
                        item1.FREQUENCYVALUE = $("#ddlMonth option:selected").text();
                    }
                    item1.PANELNAME = tbl.rows[i].children[10].textContent;
                    item1.APPLICATIONO = $('#hdnAppNo').val();
                    item1.status = status;
                    item1.MONTHNO = tbl.rows[i].children[12].textContent;
                    Items.push(item1);
                }
            }
            //checking wheither for all the villages the source village have been selected or not
            if (!isallowed) {
                bootbox.alert("Plese enter a data point value!");
                return false;
            } else {
                var message = "";
                if (status == '0') {
                    message = "Are you sure , you want to Save As Draft?";
                }
                else {
                    message = "Are you sure , you want to Submit?";
                }
                if (validate()) {
                    bootbox.confirm({
                        size: "medium",
                        message: message,

                        callback: function (result) {
                            if (result == true) {
                                $(".my-loader").show();
                                $.ajax({
                                    url: "/BlockData/BlockDataEntryTemp",
                                    data: JSON.stringify(Items),
                                    beforeSend: function (xhr) {
                                        xhr.setRequestHeader("XSRF-TOKEN",
                                            $('input:hidden[name="__RequestVerificationToken"]').val());
                                    },
                                    type: "POST",
                                    contentType: "application/json;charset=utf-8",
                                    dataType: "json",
                                    success: function (result) {
                                        $(".my-loader").hide();
                                        if (status == '0') {
                                            bootbox.alert(result.msg, function () {
                                                window.location.href = "/BlockData/BlockDataEntryTemp?FreqId=" + result.freqid + "&Glink=" + gl + "&Plink=" + pl;
                                            });
                                        }
                                        else {
                                            window.location.href = "/BlockData/PreviewBlockDataEntry?AppNo=" + result.appno + "&Year=" + result.year + "&FreqId=" + result.freqid + "&Status=0&FreqNo=" + result.freqno + "&MonthNo=" +$("#ddlMonth").val() + "&Glink=" + gl + "&Plink=" + pl;
                                        }
                                    },
                                    error: function (result) {
                                        $(".my-loader").hide();
                                        bootbox.alert(result);
                                    }
                                });

                            }
                        }
                    });
                }
            }
        }
    }
