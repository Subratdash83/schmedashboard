@model ABP.Domain.Report.SectorPRFData
@using Microsoft.AspNetCore.Http;
@inject IHttpContextAccessor HttpContextAccessor
@inject Microsoft.AspNetCore.Hosting.IHostingEnvironment hostingEnv
@{
    ViewData["Title"] = "SectorWiseEntryPerformance";
    Layout = "~/Views/Shared/_Layout.cshtml";
    string Glink = Context.Request.Query["Glink"];
    string Plink = Context.Request.Query["Plink"];
    var year = ViewBag.year;
    var ListSectorData = ViewBag.SectorDpCountData as Task<IEnumerable<ABP.Domain.Report.SectorPRFData>>;
    var ListSectorTotalData = ViewBag.SectorTtlData as Task<IEnumerable<ABP.Domain.Report.SectorPRFData>>;
    var ListSectorYearTotalData = ViewBag.SectorTotlData as Task<IEnumerable<ABP.Domain.Report.SectorPRFData>>;
    
}
<link href="~/css/entryperformance.css" rel="stylesheet">
<style>
    .sum-row {
        background: #ebebeb;
    }
</style>
<div class="page-title">
    <div class="title-details">
        <nav aria-label="breadcrumb">
            <ol class="breadcrumb p-0 mb-0 bg-transparent" id="navigation">
                <li class="breadcrumb-item"><a href="../Home/MapData" title="Home"><i class="fa fa-home"></i></a></li>
                <li class="breadcrumb-item">MIS Report </li>
                <li class="breadcrumb-item font-weight-bold">Sector wise Entry Performance</li>
            </ol>
        </nav>
    </div>
</div>
<div class="row">
    <div class="col-md-12 col-sm-12">
        <div class="card">

            <div class="card-header">

                <div class="indicatorslist">
                    <p class="ml-2">(*) Indicates mandatory </p>
                </div>
            </div>
            <div class="card-body">
                <form method="post" id="search">
                    <div class="form-group row">
                        <div class="col-12 col-md-6 col-lg-3 col-xl-3">
                            <label class="control-label" for="demo-email-input">Year<span class="text-danger">*</span></label>
                            <select class="form-control" id="ddlYear" asp-for="Year" asp-items="@(new SelectList(ViewBag.Years, "FREQUENCYNO", "FREQUENCYVALUE" ))">
                                <option value=0>--Select--</option>
                            </select>
                        </div>
                        <div class="col-12 col-md-6 col-lg-3 col-xl-3">
                            <input type="hidden" id="hdnDistrict" value="" />
                            <label class="control-label" for="demo-email-input">District<span class="text-danger">*</span></label>
                            <select class="form-control" id="ddlDistrict" asp-for="DISTRICTID" onchange="BindBlockData(0)" asp-items="@(new SelectList(ViewBag.DistrictData, "BLOCK_CODE", "BLOCK_NAME" ))">
                                <option value=0>---Select---</option>
                            </select>
                        </div>
                        <div class="col-12 col-md-6 col-lg-3 col-xl-3">
                            <input type="hidden" id="hdnDistrict" value="" />
                            <label class="control-label" for="demo-email-input">Block<span class="text-danger">*</span></label>
                            <div id="divblock">
                                <select class="form-control" id="ddlBlock" name="Blockid">
                                    <option value="0">---Select---</option>
                                </select>
                            </div>
                        </div>
                        <div id="ddindicatorhide" class="col-12 col-md-3 col-xl-3">
                            <br />
                            <button id="btnSearch" class="btn btn-primary"> Search</button>

                        </div>



                    </div>

                </form>
                <div class="legend-section mb-3 mt-4">

                    @if (ListSectorData != null)
                    {
                        
                        <div class='legend-box legend-box2'>
                           
                                <a href='#' class='text-success  mb-lg-0' id="gcheck">  <span class='back-green1'></span> <strong>100% Completed </strong> </a>
                                <a href='#' class='text-warning mb-lg-0' id="ycheck">  <span class='back-yellow1'></span>  <strong> &gt;50% Completed</strong> </a>
                                <a href='#' class='text-danger mb-lg-0' id="rcheck">  <span class='back-red1'></span>  <strong>&lt; 50% Completed </strong></a>
                           
                           

                        </div>
                        <div>
                            <a href="javascript:PrintPagePF();void(0)" title="Print" id="printIcon" data-toggle="tooltip" data-placement="top" class="btn btn-sm btn-default float-md-right ml-1 "><i class="icon-printer1"></i></a>

                            <a href="javascript:generateExcelD();void(0)" title="Export to Excel" id="printIconD" data-toggle="tooltip" data-placement="top" class="btn btn-sm btn-default float-md-right "><i class="icon-excel-file"></i></a>
                        </div>

                           
                        
