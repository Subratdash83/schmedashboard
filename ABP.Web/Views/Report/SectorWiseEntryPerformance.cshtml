<script>
    function getvisible() {
        var vtable = []
        var headrow = $('#tbldata thead').find('tr').html();
        vtable.push(headrow);
        $('#tbldata tbody').find('tr:visible').each(function () {
            var temprow = '';
            if ($(this).css('display') == 'table-row') {
                temprow += '<tr>' + $(this).html() + '</tr>';
            }
            vtable.push(temprow);
        });
        return vtable;
    }
