
@{
    ViewData["Title"] = "DistrictDashboard";
    Layout = "~/Views/Shared/_Layout.cshtml";
    var BlockWiseDP=ViewBag.BlockWiseData as List<ABP.Domain.Dashboard.DashboardNew>;
    var SectorBlockWiseDP = ViewBag.SectorDataBlockWise as List<ABP.Domain.Dashboard.DashboardNew>;
}

<link href="~/css/collector.css" rel="stylesheet" />
<style>
    .toptable-heading {
        font-size: 14px !important;
    }

    .lowertable-heading {
        font-size: 12px !important;
    }
</style>
<div class="card">
    <div class="card-header">
        <div class="card-title">
            <h5>Collector Dashboard</h5>
        </div>
        <ul class="filter-points">
            <li>
                <select class="form-control active" id="ddlDistrict" name="BLOCK_CODE" asp-items="@(new SelectList(ViewBag.District, "BLOCK_CODE", "BLOCK_NAME"))">
                </select>
            </li>
            <li>
                <select class="form-control month-select" id="ddlMonth" name="Month" asp-items="@(new SelectList(ViewBag.Month, "FREQUENCYNO", "FREQUENCYVALUE", ViewBag.Month1))" onchange="MonthChange();"></select>
            </li>
            <li>
                <select class="form-control" id="ddlYear" name="Year" asp-items="@(new SelectList(ViewBag.Years, "FREQUENCYNO", "FREQUENCYVALUE",ViewBag.Year1))" onchange="YearChange();"></select>
            </li>
        </ul>
    </div>
    <div class="card-body">
        <div class="collector-dashboard">

            <div class="row">
                <div class="col-md-3">
                    <p class="title-text">Blocks  <button class="btn district-btn" id="district-btn" href="#">Details </button></p>
                    <div class="responsive-mobile">
                        <div class="tab-scroll">
                            <div class="inner-scroll">

                                <div class="nav flex-column nav-pills district-tab" id="v-pills-tab" role="tablist" aria-orientation="vertical">
                                    @foreach (var Block in BlockWiseDP)
                                    {<a class="nav-link" id="v-@Block.BLOCKID" data-toggle="pill" href="#v-pills-home" role="tab" aria-controls="v-pills-home" aria-selected="true" onclick="showSectorData(@Block.BLOCKID);">
                                            <div class="progress-container">
                                                <div class="progress-text">
                                                    <h5>@Block.SUM_ENTERED%</h5>
                                                    <p>@Block.BLOCKNAME</p>
                                                </div>
                                                <div class="progress" data-percentage="@Block.SUM_ENTERED">
                                                    <span class="progress-left">
                                                        <span class="progress-bar progresssuccess-bg"></span>
                                                    </span>
                                                    <span class="progress-right">
                                                        <span class="progress-bar progresssuccess-bg"></span>
                                                    </span>
                                                </div>
                                            </div>
                                        </a>
                                    }
                                </div>
                            </div>

                        </div>
                    </div>
                    
                </div>
                <div class="col-md">
                    @*<ul class="indicatoe-legend">
                        <li><span class="font-weight-bold">DP</span>- Data Point</li>
                        <li><span class="font-weight-bold success-text">MD</span>- Monthly Data Point</li>
                        <li><span class="font-weight-bold danger-text">YD</span>- Yearly Data Point</li>
                    </ul>*@
