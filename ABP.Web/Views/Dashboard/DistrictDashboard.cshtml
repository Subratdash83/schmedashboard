<script>

    $(document).ready(function () {
        var ID = getUrlVars()["BlockId"];
        if (ID == undefined) {
            @*showSectorData(@ViewBag.TopBlock);*@
            $("#v-"+@ViewBag.TopBlock+"").addClass("active");
        }
        else {
            $("#v-"+ID+"").addClass("active");
        }
        getTotal();
        $("#district-btn").click(function () {
            $(".responsive-mobile").toggle();
        });
    });
    function MonthChange() {
        var ID = getUrlVars()["BlockId"];
        var Blockid = 0;
        if (ID == undefined) {
            Blockid = @ViewBag.TopBlock;
        } else {
            Blockid = ID;
        }
        $(".my-loader").show();
        location.href = "/Dashboard/DistrictDashboard?BlockId=" + Blockid + "&Year=" + $('#ddlYear').val() + "&month=" + $('#ddlMonth').val();
    }
    function YearChange() {
        var ID = getUrlVars()["BlockId"];
        var Blockid = 0;
        if (ID == undefined) {
            Blockid = @ViewBag.TopBlock;
        } else {
            Blockid = ID;
        }
        $(".my-loader").show();
        location.href = "/Dashboard/DistrictDashboard?BlockId=" + Blockid + "&Year=" + $('#ddlYear').val() + "&month=" + $('#ddlMonth').val();
    }
    function showSectorData(id) {
        $(".my-loader").show();
        location.href = "/Dashboard/DistrictDashboard?BlockId=" + id + "&Year=" + $('#ddlYear').val() + "&month=" + $('#ddlMonth').val();
    };
    function getTotal() {
        let t1 = 0, t2 = 0, t3 = 0, t4 = 0, t5 = 0;
        $("#main-table tbody").find("tr").each(function () {
            t1 = t1 + parseInt($(this).find('td:eq(2)').text());
            t2 = t2 + parseInt($(this).find('td:eq(3)').text());
            t3 = t3 + parseInt($(this).find('td:eq(4)').text());
            t4 = t4 + parseInt($(this).find('td:eq(5)').text());
            t5 = t5 + parseInt($(this).find('td:eq(6)').text());
        });
        var html = "<tr class='primary-bg'><td colspan ='2' class='pl-5 fw-600'> Total</td><td class='fw-600 text-center'>" + t1 + "</td><td class='success-text fw-600 text-center'>" + t2 + "</td><td class='danger-text fw-600 text-center'>" + t3 + "</td><td class='success-text fw-600 text-center'>" + t4 + "</td><td class='danger-text fw-600 text-center'>" + t5 + "</td></tr >";
        $("#tfoot").append(html);
    }
    function MonthYearEntered(DPCOUNT, SECTORID, FREQUENCYID) {
        var ID = getUrlVars()["BlockId"];
        var Blockid = 0;
        if (ID == undefined) {
            Blockid = @ViewBag.TopBlock;
        } else {
            Blockid = ID;
        }
        if (DPCOUNT == 0) {
            bootbox.alert("Data Not Entered For This Month");
        }
        else {
            window.open("/BlockData/BlockWiseDPView?FreqId=" + FREQUENCYID + "&sector=" + SECTORID  + "&FreqNo=" + @ViewBag.Month1 + "&year=" + @ViewBag.Year1+"&blockid=" + Blockid , '_blank', 'width=1000');
        }
    }
</script>
