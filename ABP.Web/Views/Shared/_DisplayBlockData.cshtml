@{
    var ListData = ViewBag.BlockData as List<ABP.Domain.BlockData.BlockData>;
}
<div class="card-body"> 
    <div class="table-responsive col-sm-12 mt-3">
        <table class="table table-hover">
            <thead>
                <tr>
                    <th>Sl#</th>
                    <th>Datapoint</th>
                    <th class="text-right">Value</th>
                </tr>
            </thead>
            <tbody class="tbodySector" id="tbody">
                @{
                    int Count = 1;
                }

                @foreach (var item in ListData)
                {
                    <tr>
                        <td>@Count</td>
                        <td>@item.CONTROLNAME</td>
                        <td class="text-right">
                            @if (item.APPLICATIONNO != null)
                            {
                                @item.CONTROLVALUE
                            }
                            else
                            {
                                <span style="color:forestgreen">Not Entered</span>
                            }
                        </td>
                    </tr>
                    Count++;
                }
            </tbody>
        </table>
    </div>
</div>



